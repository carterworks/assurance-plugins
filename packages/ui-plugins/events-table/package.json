{
  "name": "events-table",
  "version": "1.0.0",
  "description": "Plugin that shows events in a tabular format",
  "main": "index.js",
  "author": "Adobe Systems",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "build": "npm-run-all --npm-path npm --parallel --aggregate-output build:src build:defs",
    "build:src": "quarry-scripts rollup",
    "build:defs": "quarry-scripts tsc --declaration --emitDeclarationOnly -p .",
    "start": "parcel src/index.html --cache-dir ./.parcel-cache  --https --cert $CERT_PATH --key $KEY_PATH --host dev.adobe.com -p 4321 --no-cache"
  },
  "dependencies": {
    "@adobe/react-spectrum": "3.31.0",
    "@assurance/plugin-bridge-provider": "1.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^2.10.3"
  }
}
